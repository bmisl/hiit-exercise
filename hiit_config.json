  "strength_metadata": {
    "Goblet Squat": {
      "sets": "3-4",
      "reps": "6-12",
      "cues": "Hold one dumbbell vertically against chest; Feet shoulder-width; Sit down between hips; Knees track over toes; Keep torso tall; Depth: thighs at least parallel"
    },
    "Romanian Deadlift": {
      "sets": "3-4",
      "reps": "6-10",
      "cues": "Soft knees; Hinge at hips (push hips back); Keep spine neutral; Lower until hamstrings stretch; Drive hips forward to stand; This is a hinge, not a squat."
    },
    "Bulgarian Split Squat": {
      "sets": "3",
      "reps": "8-12 each leg",
      "cues": "Rear foot elevated; Front foot far enough forward for vertical shin; Drop straight down; Slight torso lean is fine; Drive through mid-foot"
    },
    "Calf Raises": {
      "sets": "3-4",
      "reps": "12-20",
      "cues": "Full stretch at bottom; Pause 1 second at top; Slow eccentric"
    },
    "Hanging Leg Raise": {
      "sets": "3",
      "reps": "8-15",
      "cues": "Control the swing; Lift legs as high as possible; Slow controlled lower"
    },
    "DB Squat or Deadlift": {
      "sets": "4",
      "reps": "5-8",
      "cues": "Cycle 1: Squat focus; Cycle 2: Deadlift focus; Heavier than leg day."
    },
    "Dumbbell Bench Press": {
      "sets": "3-4",
      "reps": "6-10",
      "cues": "Feet planted; Shoulder blades pulled back; Lower with control; Press slightly inward at top"
    },
    "One-Arm Dumbbell Row": {
      "sets": "3-4",
      "reps": "8-12 each side",
      "cues": "One knee + hand on bench; Neutral spine; Pull elbow toward hip (not shoulder)"
    },
    "Overhead Press": {
      "sets": "3",
      "reps": "6-10",
      "cues": "Brace core; Press slightly back, not forward; Avoid overextending lower back"
    },
    "Pull-Ups": {
      "sets": "3-4",
      "reps": "Near failure",
      "cues": "Full range of motion; Control the descent; Add weight or use band as needed"
    },
    "Incline Dumbbell Press": {
      "sets": "3-4",
      "reps": "8-12",
      "cues": "Targets upper chest; Feet planted; Controlled movement"
    },
    "Chest-Supported Row": {
      "sets": "3-4",
      "reps": "8-12",
      "cues": "Reduces lower back fatigue; Pull elbows back; Squeeze shoulder blades"
    },
    "Light Overhead Press": {
      "sets": "3",
      "reps": "8-12",
      "cues": "Lighter than full-body day; Focus on form and control"
    },
    "Lateral Raises": {
      "sets": "3",
      "reps": "12-20",
      "cues": "Light weight, strict form; Up to shoulder height; Control the descent"
    },
    "Arms (Curls/Dips)": {
      "sets": "2-3 each",
      "reps": "10-15",
      "cues": "DB curls + Tricep extensions or Dips; Focus on mind-muscle connection"
    }
  },
